.icon-saoma:before { content: "\e604"; }

.icon-churuku:before { content: "\e61e"; }



page {
  background-color: #f8f8f8;
  font-size: 16px;
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
} 

.page__hd {
  padding: 40px;
}

.page__bd {
  padding-bottom: 40px;
}

.page__bd_spacing {
  padding-left: 15px;
  padding-right: 15px;
}

.page__ft {
  padding-bottom: 10px;
  text-align: center;
}

.page__title {
  text-align: left;
  font-size: 20px;
  font-weight: 400;
}

.page__desc {
  margin-top: 5px;
  color: #888;
  text-align: left;
  font-size: 14px;
}

.weui-badge-primary {
  border: solid #1aad19 1px;
  color: #1aad19;
  background-color: #f8f8f8;
}

.wx-text-success{
  color:#1aad19
}

.wx-text-primary{
  color:#1E9EEA
}

.wx-text-warning{
  color:#FDCD4A
}

.shop-image {
  width: 100%;
  height: 300rpx;
}

.icon-image {
  margin: 0rpx 10rpx 0rpx 10rpx;
  width: 50rpx;
  height: 50rpx;
}

.icon-35 {
  width: 35rpx;
  height: 35rpx;
}

.icon-40 {
  width: 40rpx;
  height: 40rpx;
}

.wx-60-text{
  font-size:60rpx;
}

.wx-120-text{
  font-size:120rpx;
}

.wx-16-text{
  font-size:16rpx;
}
.wx-20-text{
  font-size:20rpx;
}
.wx-30-text{
  font-size:30rpx;
}
